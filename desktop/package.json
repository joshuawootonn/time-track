{
  "name": "timetrack",
  "author": {
    "name": "Joshua Wootonn",
    "email": "joshuawootonn@gmail.com",
    "url": "http://www.joshuawootonn.com"
  },
  "repository": "joshuawootonn/time-track",
  "description": "Time Managment & Analytics",
  "version": "3.0.47",
  "url": "http://www.joshuawootonn.com",
  "license": "MIT",
  "private": false,
  "build": {
    "files": [
      "build/**"
    ],
    "appId": "com.electron.timetrack",
    "win": {
      "target": "nsis",
      "icon": "build/favicon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/favicon.ico"
    },
    "mac": {
      "target": "dmg",
      "hardenedRuntime": true,
      "gatekeeperAssess": true,
      "extendInfo": {
        "NSAppleEventsUsageDescription": "Let me use Apple Events.",
        "NSCameraUsageDescription": "Let me use the camera.",
        "NSScreenCaptureDescription": "Let me take screenshots."
      }
    },
    "dmg": {
      "iconSize": 100,
      "contents": [
        {
          "x": 255,
          "y": 85,
          "type": "file"
        },
        {
          "x": 253,
          "y": 325,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "width": 500,
        "height": 500
      }
    },
    "publish": [
      {
        "provider": "github",
        "owner": "joshuawootonn",
        "repo": "time-track",
        "publishAutoUpdate": true
      }
    ],
    "directories": {
      "buildResources": "build",
      "output": "release"
    }
  },
  "main": "./build/electron.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "^3.0.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "12.7.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.4",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^3.0.1",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.0.6",
    "exceljs": "^1.6.2",
    "formik": "^1.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "normalizr": "^3.2.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.0.4",
    "react-simple-keyboard": "^1.21.3",
    "react-virtualized": "9.22.3",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "swr": "^2.2.0",
    "typescript": "^5.1.6",
    "yup": "^1.2.0"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "electron-build": "electron-builder",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "pack-win": "pnpm build && pnpm electron-build --x64 --win",
    "pack-mac": "pnpm build && pnpm electron-build --x64 --mac",
    "dist-win": "pnpm build && pnpm electron-build  --x64 --win -p always",
    "dist-mac": "pnpm build && pnpm electron-build  --x64 --mac -p always",
    "electron": "electron .",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "electron": "^21.1.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.1.1",
    "jest": "^29.7.0",
    "postcss": "^8.4.25",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.3",
    "vite": "^4.4.3"
  },
  "resolutions": {
    "react-virtualized": "git+https://git@github.com/remorses/react-virtualized-fixed-import.git#9.22.3"
  }
}
